{
  "name": "hub-vue",
  "version": "0.1.0",
  "description": "Utility page to check current warframe status",
  "author": "Warframe Community Developers",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:raw": "npx lint .",
    "updateWorldstate": "node ./initialWorldstateUpdater.js"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "bootswatch": "^4.3.1",
    "css-loader": "^1.0.1",
    "image-webpack-loader": "^4.6.0",
    "leaflet": "^1.3.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.3.0",
    "register-service-worker": "^1.6.1",
    "vue": "^2.6.6",
    "vue-analytics": "^5.16.2",
    "vue-native-notification": "^1.0.5",
    "vue-raven": "^1.0.3",
    "vue-responsive-grid-layout": "^1.1.3",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.6",
    "vue2-leaflet": "^1.2.3",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-indent": "warning",
      "semi": 1,
      "arrow-parens": 1,
      "quotes": [
        1,
        "single"
      ]
    }
  },
  "eslintIgnore": [
    "src/assets/**.*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.1"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  }
}
