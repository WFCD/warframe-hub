{
  "name": "hub-vue",
  "version": "0.1.0",
  "private": true,
  "description": "Utility page to check current warframe status",
  "author": "Warframe Community Developers",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:raw": "npx lint .",
    "start": "node ./server.js",
    "updateWorldstate": "node ./initialWorldstateUpdater.js"
  },
  "dependencies": {
    "express": "^4.16.4",
    "hooper": "^0.1.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@ky-is/vue-cli-plugin-tailwind": "^1.7.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-pwa": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "css-loader": "^2.1.1",
    "image-webpack-loader": "^4.6.0",
    "leaflet": "^1.4.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-fetch": "^2.4.1",
    "postcss-preset-env": "^6.6.0",
    "register-service-worker": "^1.6.2",
    "tailwindcss": "next",
    "tailwindcss-elevation": "^0.2.0",
    "vue": "^2.6.10",
    "vue-analytics": "^5.16.4",
    "vue-native-notification": "^1.0.5",
    "vue-raven": "^1.0.3",
    "vue-responsive-grid-layout": "^1.1.8",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vue2-leaflet": "^2.1.1",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-indent": "warning",
      "semi": 1,
      "arrow-parens": 1,
      "quotes": [
        1,
        "single"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.1"
  },
  "eslintIgnore": [
    "src/assets/**.*"
  ],
  "heroku-run-build-script": true
}
