<script>
  /* global $ */
  const components = ['event', 'acolytes', 'cetus', 'vallis', 'earth', 'bounties',
    'alerts', 'news', 'invasions', 'reset', 'sortie', 'fissures', 'baro',
    'darvo', 'deals'];

  // Wait until page is ready to display
  document.getElementsByTagName("html")[0].style.visibility = "hidden";
  $(document).ready(function() {
    document.getElementsByTagName("html")[0].style.visibility = "visible";

    // Theme loading
    if (!localStorage.getItem('mode')) {
      localStorage.setItem('mode', 'night');
    }
    if (!$('html').hasClass(localStorage.getItem('mode'))) {
      setTheme(localStorage.getItem('mode'));
    }
  });

  const sums = JSON.parse("{{json sums}}".replace(/&quot;/ig, '"'));
</script>
